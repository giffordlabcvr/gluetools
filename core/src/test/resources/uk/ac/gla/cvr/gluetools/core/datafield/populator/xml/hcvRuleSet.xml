<dataFieldPopulator>
	<gbPrimaryAccession>
		<fieldPopulator fieldName="GB_PRIMARY_ACCESSION"/>
	</gbPrimaryAccession>
	<gbOrganism>
		<fieldPopulator fieldName="GB_ORGANISM">
			<valueConverter>
				<matchPattern>.*Hepatitis C [vV]irus.*</matchPattern>
				<outputPattern>Hepatitis C virus</outputPattern>
			</valueConverter>
		</fieldPopulator>
		<fieldPopulator fieldName="GB_RECOMBINANT">
				<matchPattern>[Rr]ecombinant</matchPattern>
				<outputPattern>true</outputPattern>
		</fieldPopulator>
	</gbOrganism>
	<gbSeqDivision>
		<fieldPopulator fieldName="GB_PATENT_RELATED">
			<matchPattern>PAT</matchPattern>
			<outputPattern>true</outputPattern>
		</fieldPopulator>
	</gbSeqDivision>
	<gbFeature key="source">
		<gbQualifier name="isolate">
			<fieldPopulator fieldName="GB_ISOLATE"/>
		</gbQualifier>
		<gbQualifier name="note">
			<fieldPopulator fieldName="GB_GENOTYPE">
				<matchPattern>genotype:?\s+([1-9])</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_SUBTYPE">
				<matchPattern>genotype:?\s+[1-9]([a-z]+)</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_RECOMBINANT">
				<matchPattern>[Rr]ecombinant</matchPattern>
				<outputPattern>true</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_GENOTYPE">
				<matchPattern>subtype\s+([1-9])</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_SUBTYPE">
				<matchPattern>subtype\s+[1-9]([a-z]+)</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
		</gbQualifier>
		<gbQualifier name="genotype">
			<fieldPopulator fieldName="GB_GENOTYPE">
				<matchPattern>([1-9])</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_SUBTYPE">
				<matchPattern>[1-9]([a-z]+)</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
		</gbQualifier>

	<!-- this will genotype about 70 odd sequences, not sure if it is valid. -->
		<gbQualifier name="serotype">
			<fieldPopulator fieldName="GB_GENOTYPE">
				<matchPattern>([1-9])</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_SUBTYPE">
				<matchPattern>[1-9]([a-z]+)</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
		</gbQualifier>
	
		<gbQualifier name="type">
			<fieldPopulator fieldName="GB_GENOTYPE">
				<matchPattern>([1-9])</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_SUBTYPE">
				<matchPattern>[1-9]([a-z]+)</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
		</gbQualifier>
		<gbQualifier name="subtype">
			<fieldPopulator fieldName="GB_GENOTYPE">
				<matchPattern>([1-9])[a-z]+</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_SUBTYPE">
				<matchPattern>([a-z]+)</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
		</gbQualifier>
		<gbQualifier name="organism">
			<fieldPopulator fieldName="GB_GENOTYPE">
				<matchPattern>subtype\s+([1-9])</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_SUBTYPE">
				<matchPattern>subtype\s+[1-9]([a-z]+)</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
		</gbQualifier>
		<gbQualifier name="strain">
			<fieldPopulator fieldName="GB_GENOTYPE">
				<matchPattern>([1-9])</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_SUBTYPE">
				<matchPattern>[1-9]([a-z]+)</matchPattern>
				<outputPattern>${1}</outputPattern>
			</fieldPopulator>
			<fieldPopulator fieldName="GB_RECOMBINANT">
				<matchPattern>[Rr]ecombinant</matchPattern>
				<outputPattern>true</outputPattern>
			</fieldPopulator>		
			<fieldPopulator fieldName="GB_RECOMBINANT">
				<matchPattern>RF</matchPattern>
				<outputPattern>true</outputPattern>
			</fieldPopulator>		
		</gbQualifier>
	</gbFeature>
</dataFieldPopulator>