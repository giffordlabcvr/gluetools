    set metatag CONJUNCT_NAME_1 463L
    set metatag CONJUNCT_NAME_2 467A
    exit

  create variation 463I+467A -t conjunction
  variation 463I+467A
    set metatag CONJUNCT_NAME_1 463I
    set metatag CONJUNCT_NAME_2 467A
    exit
        
  exit