<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/3.0/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/3.0/modelMap http://cayenne.apache.org/schema/3.0/modelMap.xsd"
	 project-version="6">
	<property name="defaultPackage" value="uk.ac.gla.cvr.gluetools.core.datamodel"/>
	<property name="defaultSuperclass" value="uk.ac.gla.cvr.gluetools.core.datamodel.GlueDataObject"/>
	<db-entity name="FIELD">
		<db-attribute name="NAME" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="50"/>
		<db-attribute name="PROJECT" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="50"/>
		<db-attribute name="TYPE" type="VARCHAR" isMandatory="true" length="20"/>
	</db-entity>
	<db-entity name="MODULE">
		<db-attribute name="CONFIG" type="BLOB" isMandatory="true"/>
		<db-attribute name="NAME" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="50"/>
		<db-attribute name="PROJECT" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="50"/>
	</db-entity>
	<db-entity name="PROJECT">
		<db-attribute name="DESCRIPTION" type="VARCHAR" length="200"/>
		<db-attribute name="NAME" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="50"/>
	</db-entity>
	<db-entity name="SEQUENCE">
		<db-attribute name="DATA" type="BLOB" isMandatory="true"/>
		<db-attribute name="FORMAT" type="VARCHAR" isMandatory="true" length="20"/>
		<db-attribute name="SEQUENCE_ID" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="50"/>
		<db-attribute name="SOURCE" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="50"/>
	</db-entity>
	<db-entity name="SOURCE">
		<db-attribute name="NAME" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="50"/>
		<db-attribute name="PROJECT" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="50"/>
	</db-entity>
	<obj-entity name="Field" className="uk.ac.gla.cvr.gluetools.core.datamodel.Field" dbEntityName="FIELD" superClassName="uk.ac.gla.cvr.gluetools.core.datamodel.GlueDataObject">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="NAME"/>
		<obj-attribute name="type" type="java.lang.String" db-attribute-path="TYPE"/>
	</obj-entity>
	<obj-entity name="Module" className="uk.ac.gla.cvr.gluetools.core.datamodel.Module" dbEntityName="MODULE" superClassName="uk.ac.gla.cvr.gluetools.core.datamodel.GlueDataObject">
		<obj-attribute name="config" type="byte[]" db-attribute-path="CONFIG"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="NAME"/>
	</obj-entity>
	<obj-entity name="Project" className="uk.ac.gla.cvr.gluetools.core.datamodel.Project" dbEntityName="PROJECT" superClassName="uk.ac.gla.cvr.gluetools.core.datamodel.GlueDataObject">
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="DESCRIPTION"/>
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="NAME"/>
	</obj-entity>
	<obj-entity name="Sequence" className="uk.ac.gla.cvr.gluetools.core.datamodel.Sequence" dbEntityName="SEQUENCE" superClassName="uk.ac.gla.cvr.gluetools.core.datamodel.GlueDataObject">
		<obj-attribute name="data" type="byte[]" db-attribute-path="DATA"/>
		<obj-attribute name="format" type="java.lang.String" db-attribute-path="FORMAT"/>
		<obj-attribute name="sequenceID" type="java.lang.String" db-attribute-path="SEQUENCE_ID"/>
	</obj-entity>
	<obj-entity name="Source" className="uk.ac.gla.cvr.gluetools.core.datamodel.Source" dbEntityName="SOURCE" superClassName="uk.ac.gla.cvr.gluetools.core.datamodel.GlueDataObject">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="NAME"/>
	</obj-entity>
	<db-relationship name="project" source="FIELD" target="PROJECT" toMany="false">
		<db-attribute-pair source="PROJECT" target="NAME"/>
	</db-relationship>
	<db-relationship name="project" source="MODULE" target="PROJECT" toMany="false">
		<db-attribute-pair source="PROJECT" target="NAME"/>
	</db-relationship>
	<db-relationship name="fields" source="PROJECT" target="FIELD" toDependentPK="true" toMany="true">
		<db-attribute-pair source="NAME" target="PROJECT"/>
	</db-relationship>
	<db-relationship name="modules" source="PROJECT" target="MODULE" toDependentPK="true" toMany="true">
		<db-attribute-pair source="NAME" target="PROJECT"/>
	</db-relationship>
	<db-relationship name="sources" source="PROJECT" target="SOURCE" toDependentPK="true" toMany="true">
		<db-attribute-pair source="NAME" target="PROJECT"/>
	</db-relationship>
	<db-relationship name="source" source="SEQUENCE" target="SOURCE" toMany="false">
		<db-attribute-pair source="SOURCE" target="NAME"/>
	</db-relationship>
	<db-relationship name="project" source="SOURCE" target="PROJECT" toMany="false">
		<db-attribute-pair source="PROJECT" target="NAME"/>
	</db-relationship>
	<db-relationship name="sequences" source="SOURCE" target="SEQUENCE" toDependentPK="true" toMany="true">
		<db-attribute-pair source="NAME" target="SOURCE"/>
	</db-relationship>
	<obj-relationship name="project" source="Field" target="Project" deleteRule="Nullify" db-relationship-path="project"/>
	<obj-relationship name="project" source="Module" target="Project" deleteRule="Nullify" db-relationship-path="project"/>
	<obj-relationship name="fields" source="Project" target="Field" deleteRule="Cascade" db-relationship-path="fields"/>
	<obj-relationship name="modules" source="Project" target="Module" deleteRule="Cascade" db-relationship-path="modules"/>
	<obj-relationship name="sources" source="Project" target="Source" deleteRule="Cascade" db-relationship-path="sources"/>
	<obj-relationship name="source" source="Sequence" target="Source" deleteRule="Nullify" db-relationship-path="source"/>
	<obj-relationship name="project" source="Source" target="Project" deleteRule="Nullify" db-relationship-path="project"/>
	<obj-relationship name="sequences" source="Source" target="Sequence" deleteRule="Cascade" db-relationship-path="sequences"/>
</data-map>
