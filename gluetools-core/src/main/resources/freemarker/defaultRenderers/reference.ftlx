<#include "/utils/glueDocUtils.ftlx">

<reference>
	<name>${name}</name>
	<renderedName>${getRenderedName()}</renderedName>
	<sequence>
		<sourceName>${sequence.source.name}</sourceName>
		<sequenceID>${sequence.sequenceID}</sequenceID>
	</sequence>
<#list alignmentsWhereRefSequence as alignment>
	<constrainsAlignment glueType="Object[]">
		<name>${alignment.name}</name>
		<renderedName>${alignment.getRenderedName()}</renderedName>
	</constrainsAlignment>
</#list>
</reference>