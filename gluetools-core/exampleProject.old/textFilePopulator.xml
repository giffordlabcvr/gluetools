<textFilePopulator>
	<columnDelimiterRegex>,</columnDelimiterRegex>
	<textFileColumn>
		<identifier>true</identifier>
		<number>0</number>
		<fieldName>sequenceID</fieldName>
	</textFileColumn>
	<textFileColumn>
		<number>1</number>
		<fieldName>custom_field_x</fieldName>
	</textFileColumn>
	<textFileColumn>
		<number>2</number>
		<!-- Null existing values will always be overwritten. 
		     If overwriteExistingNonNull is set to true, non-null existing field values will also be overwritten. 
		     Default if omitted is true -->
		<overwriteExistingNonNull>true</overwriteExistingNonNull>
		<!-- If overwriteWithNewNull is set to true, the field value will be updated, even if the new value is null.
		     Default if omitted is false -->
		<overwriteWithNewNull>false</overwriteWithNewNull>
		<fieldName>custom_field_y</fieldName>
		<nullRegex>n/a</nullRegex>
	</textFileColumn>
</textFilePopulator>